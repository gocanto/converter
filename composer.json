{
  "license": "MIT",
  "name": "gocanto/converter",
  "homepage": "https://github.com/gocanto/converter",
  "type": "php-bundle",
  "description": "A simple currency converter library.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "php",
    "currency",
    "converter"
  ],
  "authors": [
    {
      "name": "Gustavo Ocanto",
      "email": "gustavoocanto@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/gocanto/converter",
    "issues": "https://github.com/gocanto/converter/issues"
  },
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Gocanto\\Converter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gocanto\\Converter\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
